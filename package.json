{
  "name": "react-boilerplate",
  "private": true,
  "version": "1.0.0",
  "description": "Boilerplate for React-redux apps",
  "main": "index.js",
  "scripts": {
    "assets": "cp www/* dist",
    "clean": "mkdir -p dist && rm -rf dist/*",
    "reinstall": "rm -rf dist && rm -rf node_modules && yarn install",
    "watch": "WEBPACK_ARGS=--watch npm run webpack",
    "prewebpack": "npm run clean && npm run assets",
    "webpack": "webpack $WEBPACK_ARGS",
    "build": "NODE_ENV=development npm run webpack",
    "release": "NODE_ENV=production npm run webpack",
    "test": "babel-tape-runner test.js"
  },
  "dependencies": {
    "immutability-helper": "^2.0.0",
    "material-ui": "^0.16.5",
    "react": "^15.4.1",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-tape-runner": "^2.0.1",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-runtime": "^6.11.6",
    "css-loader": "0.24.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "normalize.css": "^4.2.0",
    "react-addons-css-transition-group": "^15.3.1",
    "react-addons-update": "^15.3.1",
    "react-hot-loader": "1.3.0",
    "tape": "^4.6.2",
    "webpack": "^1.13.3"
  }
}
